        <script src="/js/bootstrap.js"></script>
        <script src="/js/svg-loader.js"></script>
        <% if (isAuthentificated) { %>
            <script>
                var id = '<%= user.id %>';
                var token = '<%= user.accessToken %>';
            </script>
            <script src="/js/cli-function.js"></script>
        <% } %>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
        <script>
                toastr.options = {
                    "closeButton": false,
                    "debug": false,
                    "newestOnTop": false,
                    "progressBar": true,
                    "positionClass": "toast-bottom-right",
                    "preventDuplicates": false,
                    "onclick": null,
                    "showDuration": "300",
                    "hideDuration": "1000",
                    "timeOut": "10000",
                    "extendedTimeOut": "1000",
                    "showEasing": "swing",
                    "hideEasing": "linear",
                    "showMethod": "fadeIn",
                    "hideMethod": "fadeOut"
                };
                <% if (error != "") { %>
                    toastr.error('<%= error %>');
                <% } %>
                <% if (success != "") { %>
                    toastr.success('<%= success %>');
                <% } %>
        </script>
    </body>
</html>